#include <stdio.h>
#include <unistd.h>

int main() {
    pid_t pid = fork();
    
    if (pid == -1) {
        printf("Process creation failed\n");
        return 1;
    } 
    else if (pid == 0) {
        printf("Child process: PID = %d\n", getpid());
    } 
    else {
        printf("Parent process: PID = %d\n", getpid());
    }
    return 0;
}

Input:
none

Output:
Parent process: PID = 33283
Child process: PID = 33284
